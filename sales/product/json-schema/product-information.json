{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "catalog_id": {
      "type": "string"
    },
    "number": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "display_name": {
      "type": "string"
    },
    "quote_criteria": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "display_text": {
              "type": "string"
            },
            "type": {
              "type": "string"
            }
          },
          "required": [
            "name",
            "display_text",
            "type"
          ]
        }
      ]
    }
  },
  "required": [
    "id",
    "catalog_id",
    "number",
    "name",
    "display_name",
    "quote_criteria"
  ]
}